{
  "expo": {
    "name": "Rentzi",
    "slug": "rentzi",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.solulab.rentzi"
    },
    "android": {
      "package": "com.solulab.rentzi"
    },
    "web": {
      "bundler": "metro",
      "output": "static"
    },
    "plugins": [
      [
        "expo-router",
        {
          "origin": "https://rentzi.app/"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}